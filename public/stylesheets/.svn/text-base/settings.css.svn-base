.PrivacySelect ul {
    padding-left: 0.5em;
    list-style-type: none;
}

.PrivacySelect li {
    display: block;
    width: 250px;
}

.PrivacySelect ul ul {
    padding-left: 20px;
}

.Batch {
    padding: 0px 5px 10px 5px;
}

.BatchList {
    margin-top: 0px;
    width: 100%;
}

.BatchList tr {
    vertical-align: top;
}

.BatchList td {
    font-size: 12px;
    padding-top: 10px;
    padding-bottom: 40px;
    border-bottom: solid 3px #e6e6e6;
}

.BatchPic {
    text-align: center;
    padding: 20px 10px 10px 50px;
}

.BatchPic img {
    margin-top: 10px;
}

.BatchForm {
    width: 100%;
}

.BatchForm td {
    font-size: 12px;
    border: none;
    padding: 5px;
}

.BatchForm input {
    width: 250px;
}

.BatchForm input#submit {
    width: auto;
}

.BatchForm textarea {
    width: 250px;
    height: 140px;
}

.BatchDesc {
    font-size: 12px;
    line-height: 1.4em;
    color: #666;
    padding-bottom: 15px;
    text-align: center;
}

.BatchDesc table{
    margin: auto;
}

.BatchDesc .txt,textarea{
    width: 400px;
}

.BatchOpList {
    width: 50%;
    border-right: solid 10px #e6e6e6;
}

.BatchOpList ul {
    list-style-type: none;
    margin-top: 1em;
}

.BatchOpList li {
    margin-right: 10px;
    margin-bottom: 10px;
    float: left;
    text-align: center;
    width: 75px;
    height: 90px;
}

.BatchOptions {
    width: 50%;
    padding: 20px 10px 20px 20px;
}

.OneOption {
    padding: 10px;
    margin: 10px;
    border: solid 1px #e6e6e6;
}

.OneOption h5 {
    margin-top: 0px;
}

.AlbumSelect {
    height: 120px;
    overflow: auto;
}

.AlbumSelect ul {
    list-style-type: none;
    padding-left: 0.5em;
}

.AlbumSelect li {
    padding: 2px;
    border-bottom: 1px solid #f3f3f3;
}

#optionsTrigger {
    padding: 0.5em 1em;
    margin: 1em 16em 0 0;
    background-color: #f5f5f5;
}

#optionsMore {
    padding-left: 1em;
}

#optionsMore ul {
    list-style-type: none;
}

#optionsMore li {
    display: block;
    width: 250px;
}

.UserSecurityForm {
    margin-top: 17px;
    clear: none;
}