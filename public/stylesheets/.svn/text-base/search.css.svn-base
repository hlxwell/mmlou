.searchTag {
    line-height: 26px;
	margin-left:50px;
	margin-bottom:10px;
	width:720px;
}

.searchTag h2 {
    padding: 0;
    margin: 0;
    color: #6b9f20;
    height: 28px;
}

.searchTag .caption {
    font-weight: bold;
    color: #6c9f20;
    text-decoration: none;
}

.searchTag a {
    margin-right: 8px;
    color: #6B9F1F;
    font-size: 12px;
}

.searchTag a:link, .searchTag a:visited {
    color: #6B9F1F;
}

.searchTag a:hover {
    color: #FF3600;
    background: none;
}

.searchTag a.small {
    font-size: 12px;
}

.searchTag a.middle {
    font-size: 14px;
}

.searchTag a.big {
    font-size: 16px;
}

.searchTag a.huge {
    font-size: 18px;
    font-weight: bold;
}

.searchTag .num{
	font-family:Georgia;
	font-size: 10px;
	color: #000000;
}

.SearchTab {
    position: absolute;
    width: 740px;
	left: 10px !important;
	left: 30px;
	margin-top:-10px;
    position: relative;
    border-bottom: 3px solid #f1f1f1;
}

.SearchTab .Tabs {
    padding-left: 160px;
}

#searchForm {
    padding: 12px 20px 40px 150px;
}

#searchForm #keyword {
    width: 280px;
}

#searchForm .TipHint {
    width: 340px;
    padding: 10px;
    margin-left: 110px;
}
#searchResult {
	width:740px;
    margin-left: 20px;
}
#searchResult .Confirm {
    margin-left: 45px;
}

div.SearchPhotoList {
	position: relative;
    padding: 5px;
    width: 740px;
	margin-left: 5px !important;
	margin-left: 20px;
}

div.SearchAlbumList {
	position: relative;
    padding: 5px;    
    width: 740px;
	margin-left: 5px !important;
	margin-left: 20px;
}

div.SearchPeopleList {
    position: relative;
    padding: 5px;    
    width: 740px;
	margin-left: 5px !important;
	margin-left: 20px;
}


.SearchSetsHome {
    position: relative;
    width: 110px;
	margin-left:10px;
    text-align: left;
    float: left;
    margin-bottom: 20px;
}

.SearchSetsHome h4 {
    width: 110px;
    margin-top: 0px;
    margin-bottom: 0px;
    font-size: 12px;
    font-weight: bold;
}

.SearchSetsHome p {
    margin-top: 2px;
    font-size: 12px;
    color: #777;
}

.SearchSets {
    position: relative;
    width: 110px;
    height: 210px;
    margin-left:10px;
    text-align: left;
    float: left;
    margin-bottom: 20px;
    overflow:hidden;
}

.SearchSets h4 {
    width: 110px;
    margin-top: 0px;
    margin-bottom: 0px;
    font-size: 12px;
    font-weight: bold;
}

.SearchSets p {
    margin-top: 2px;
    font-size: 12px;
    color: #777;
}

div.SearchGroupList .GroupList li {
    list-style-type: none;
    width: 500px;
}

div.SearchGroupList .GroupList li p {
    margin-left: 70px;
}

div.SearchPeopleList .PeopleList li {
    list-style-type: none;
	margin-left:5px;
    margin-bottom: 1.5em;
    width: 220px;
    float: left;
}

.floorNum{
	margin:3px 0px 3px 5px;
	font-family:Georgia;
	font-size:12px;
	color:#666666;
	font-weight:normal;
}
