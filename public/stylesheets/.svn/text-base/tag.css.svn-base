.top5list {
	width: 100px;
    list-style: none;
	word-break: break-all;
}

.withLeftLine {
	margin-left: 10px;
	border-left: 1px solid #eeeeee;
}

#hotTagTitle {
    font-size: 14px;
    font-weight: bold;
    padding: 5px;
}

div.TagSearch {
    position: absolute;
    right: 10px;
    top: 27px;
    height: 30px;
    z-index: 4;
}

div.TagSearch #tag {
    width: 130px;
    margin-right: 0;
}

div.TagSearch #submit {
    margin-bottom: 0px;
}

span.PopularTag {
    padding: 0.3em 0.3em 0.3em 0;
    line-height: 1.5em;
}

div.TagsClouds {
    padding: 1.5em;
    background-color: #f5f5f5;
    border: 1px solid #eee;
	line-height: 30px;
}

div.PopularTags {
    padding-right: 5px;
}

div.PopularTags a:link, div.PopularTags a:visited {
    text-decoration: none;
    line-height: 28px;
    margin-right: 8px;
}

div.TaggedPhotoList {
    padding: 0.5em 0.5em 1em 1em;
    width: 500px;
}

.cotags {
    padding-left: 1em;
}

.Cotags {
    padding: 1em;
    line-height: 1.5em;
}

#thetags {
    margin-left: 0;
    margin-bottom: 10px;
}

#thetags a.Tag:link {
    color: #999;
}

#thetags a.Tag:hover {
    color: #fff;
}

#thetags ul {
    list-style-type: none;
}

#tagadder_input {
    margin-bottom: 0px;
    width: 168px
}

#tagadder_submit {
    margin-bottom: 2px;
}

#tagadder_msgr {
    padding: 4px;
    margin: 8px 0 4px 0;
    background-color: #EEF2FD;
    border: 1px solid #D3DEFA;
}

#tagadder_usertags {
    padding: 5px 0;
    line-height: 1.3em;
    color: #666;
}

a.UsedTag:link, a.UsedTag:visited, a.UsedTag:hover, a.UsedTag:active {
    color: #9CAC84;
    text-decoration: none;
    background: #ffffff;
    font-size: 12px;
}

a.UnusedTag:link, a.UnusedTag:visited, a.UnusedTag:hover, a.UnusedTag:active {
    text-decoration: none;
    background: inherit;
    font-size: 12px;
}

a.UnusedTag:hover, a.UnusedTag:active {
    text-decoration: underline;
}

.TagsEditList td {
    padding: 0.3em 1em;
}

.TagsEditList tr.hover {
    background-color: #eff9e4;
}

div.TagCategory {
    font-family: Georgia, Times;
    font-size: 48px;
    width: 35px;
    position: absolute;
    left: -50px;
    z-index: 1;
}

#tagsEditForm {
    padding-left: 2em;
}

#tagsEditForm p {
    margin: 1.2em 0 1em;
}

#tagsEditForm input#new_tags {
    font-size: 1.8em;
}

#tagsEditForm input#submit {
    font-size: 1.2em;
}

#tagsEditForm #thetag {
    padding: 7px 20px;
    font-size: 1.8em;
    font-weight: bold;
    background-color: #f5f5f5;
}

div#FeedSubscribe div.tags_feed {
    left: -200px;
}

div#userTagFeed {
    position: relative;
}

div#userTagFeed div.item {
    position: absolute;
    width: 800px;
    left: -200px;
    top: 20px;
}

div#userTagFeed a.anothr {
    float: right;
}