<%=@alertMessage %>
<%= form_tag(:controller=>:admin,:action => :editVip,:id=>params[:id]) %>
    <table cellspacing="5">
        <tr>
            <td align="right">
                开始时间
            </td>
            <td align="left">
                <%=date_select("vipinfo","start_at",:start_year=>2008,:end_year=>2020,:use_month_numbers=>true,:class=>"txt") %>
            </td>
        </tr>
        <tr>
            <td align="right">
                结束时间
            </td>
            <td align="left">
                <%=date_select("vipinfo","end_at",:start_year=>2008,:end_year=>2020,:use_month_numbers=>true,:class=>"txt") %>
            </td>
        </tr>
        <tr>
            <td align="right">
                等级
            </td>
            <td align="left">
                <%= select("vipinfo", "level", %w{0 1 2 3 4}) %>
            </td>
        </tr>
        <tr>
        	<td align="right">
            </td>
            <td align="left">
                <input type="submit" value="修改">
            </td>
        </tr>
    </table>
</form>