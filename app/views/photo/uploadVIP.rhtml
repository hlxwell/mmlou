<div id="content">
    <div id="headTitle">
        <h2><%=l("photo_label_uploadTitle")%></h2>
    </div>
    <div class="clear">
    </div>
    <div class="Help">
        <a title="<%=l("photo_label_howToUpload")%>" href="/about/help/photo.html#howtoupload"><%=l("photo_label_howToUpload")%></a>
    </div>
    <div id="rightWrapper">
        <div id="container">
        	<%=render :partial=>"uploadRule" %>
            <div id="main">
                <div id="uploadr">
                    <form id="uploadForm" action="/photo/upload" method="post" enctype="multipart/form-data">
                    	<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=7,0,19,0" width="500" height="450">
                           <param name="movie" value="/uploadVIP.swf" />
                           <param name="quality" value="high" />
                           <param name="FlashVars" value="sessionName=<%=session[:sessionName]%><%=@albumList %><%=@addToNewAlbum %><%=@currentAlbumID%>&user_id=<%=session[:user].id %>&browser=ie" />
                           <embed src="/uploadVIP.swf" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" width="500" height="450"></embed>
                        </object>
                    </form>
                </div>
            </div>
        </div>
        <div class="clear">
        </div>
    </div>
</div>