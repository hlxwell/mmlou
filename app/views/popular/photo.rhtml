<div id="content">
    <%=userHeaderMenu("popular") %>
    <div class="clear">
    </div>
    <%=render :partial=>"tab",:locals=>{:current=>"photo"}%> 
    <div id="container">
        <div class="PopularDesc">
            <%=l("popular_label_photoDesc")%>
        </div>
        <div id="popularPhotos">
        	<% if @photos.size > 0 %>
	        	<% for photo in @photos %>
	            <div class="StatStuff">
	                <div class="PopularPhoto">
	                    <a href="/<%=photo.user_id %>/photo/view/<%=photo.id %>" title="<%=photo.tags %>"><img src="<%=getBoxImageUrl(photo)%>" width="75" height="75" alt="<%=photo.tags %>" class="Photo"/></a>
	                </div>
	                <div class="PopularStat">
	                    <div class="Title">
	                        <h3><a href="/<%=photo.user_id %>/photo/view/<%=photo.id %>"><%=if photo.tags and photo.tags.size > 1 then photo.tags else "No Title" end %></a></h3>
	                    </div>
	                    <p class="Stat">
	                        &bull; <%=l("common_label_clickCount")%> <b><%=photo.clickCount %></b>
	                    </p>
	                </div>
	            </div>
	            <% end %>            
	            <div class="pages">
	                <%=page_links_full @photo_pages %>
	            </div>
			<% else %>
				<div class="Problem">
					<%=l("problem_label_noPhotosUploaded")%>
				</div>
			<% end %>
        </div>
    </div>
</div>
